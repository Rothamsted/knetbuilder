PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX bk: <http://knetminer.org/data/rdf/terms/biokno/>
PREFIX bkr: <http://knetminer.org/data/rdf/resources/>
PREFIX bka: <http://knetminer.org/data/rdf/terms/biokno/attributes/>

SELECT ?label ?propertyName ?isRelation
{
  {
		VALUES ( ?label ?propertyName ?isRelation )
		{
			( '*' dcterms:identifier false )
			( '*' bk:prefName false )
			( '*' bk:altName false )
			( '*' rdfs:label false )
			( '*' rdfs:label true )
			
			#Â New Neo 4.3 indexing at label/relation-type level
	  	( '*' '_type_' false)
	  	( '*' '_type_' true)
		}
	}
		
	UNION 
	{
		# All the Ondex-indexed properties are indexed in Neo, both for 
		# nodes and relations
		#
	  ?propertyName rdfs:subPropertyOf* bk:attribute;
					  bk:isIndexed true.
	  BIND ( '*' AS ?label )

	  { BIND ( true as ?isRelation ) }
	  UNION { BIND ( false as ?isRelation ) }
	}
}